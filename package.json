    "demo_build:ssr": "ng build --configuration demo && ng run ng-shopketo:server:demo",
    "prerender": "ng run ng-shopketo:prerender",
    "start_pwa": "npm run prod_build && http-server -p 8080 -c-1 dist/ng-shopketo/browser"
  },
