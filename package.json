  "scripts": {
    "ng": "ng",
    "start": "npm run serve:ssr",
    "build": "npm run demo_build:ssr",
    "prod_build": "ng build --configuration production",
    "staging_build": "ng build --configuration staging",
    "demo_build": "ng build --configuration demo",
    "test": "ng test",
    "lint": "ng lint",
    "lint_max": "node --max_old_space_size=8048 ./node_modules/@angular/cli/bin/ng lint",
    "e2e": "ng e2e",
    "dev:ssr": "ng run ng-shopketo:serve-ssr",
    "serve:ssr": "node dist/ng-shopketo/server/main.js",
